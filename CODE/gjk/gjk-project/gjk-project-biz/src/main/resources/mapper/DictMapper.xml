<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.inforbus.gjk.pro.mapper.DictMapper">

    <select id="getDictBytype" resultType="com.inforbus.gjk.admin.api.entity.SysDict">
		SELECT *
		FROM sys_dict s
		WHERE s.type = #{type} AND s.del_flag = "0"
	</select>

</mapper>
